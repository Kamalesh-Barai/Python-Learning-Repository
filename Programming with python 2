fruits.insert(0, "apple")
print(fruits)
fruits.insert(2, "coconut")
print(fruits)

# If we want to exclude any elments of list, we can use remove() method.

print(fruits)
fruits.remove("coconut")
print(fruits)

item = "coconut"
if item in fruits:
    fruits.remove(item)
    print(fruits)
else:
    print(item, "not in fruits")

